        }
        
        // Sync Code generieren
        function generateSyncCode() {
            const syncData = {
                version: '1.0',
                timestamp: new Date().toISOString(),
                weekPlan: currentWeekPlan,
                shoppingList: shoppingListData,
                preferences: preferences,
                weeksSincePizza: weeksSincePizza
            };
            
            // Compress to base64
            const jsonString = JSON.stringify(syncData);
            const compressed = btoa(encodeURIComponent(jsonString));
            
            // Display code
            const codeElement = document.getElementById('syncCode');
            codeElement.textContent = compressed;
            
            // Generate QR Code if library is loaded
            const qrContainer = document.getElementById('qrcode');
            qrContainer.innerHTML = ''; // Clear previous QR
            
            // Only create QR if QRCode is defined and string is not too long
            if (typeof QRCode !== 'undefined' && compressed.length < 2000) {
                new QRCode(qrContainer, {
                    text: compressed,
                    width: 200,
                    height: 200,
                    colorDark: "#764ba2",
                    colorLight: "#ffffff",
                    correctLevel: QRCode.CorrectLevel.M
                });
            } else {
                qrContainer.innerHTML = '<p style="color: #999; text-align: center;">Code zu lang f√ºr QR - bitte kopieren</p>';
            }
        }
        
        // Sync Code kopieren
        function copySyncCode() {
            const codeElement = document.getElementById('syncCode');
            const code = codeElement.textContent;
            
            if (code === 'Generiere erst einen Wochenplan...') {
                alert('‚ùå Bitte erst einen Wochenplan generieren!');
                return;
            }
            
            // Copy to clipboard
            navigator.clipboard.writeText(code).then(() => {
                // Show success
                const btn = event.target;
                const originalText = btn.innerHTML;
                btn.innerHTML = '‚úÖ Kopiert!';
                btn.style.background = '#4CAF50';
                
                // Update sync info
                const syncInfo = document.getElementById('syncInfo');
                const now = new Date();
                syncInfo.textContent = `Code erstellt: ${now.toLocaleDateString('de-CH')} um ${now.toLocaleTimeString('de-CH', {hour: '2-digit', minute: '2-digit'})}`;
                
                setTimeout(() => {
                    btn.innerHTML = originalText;
                    btn.style.background = '';
                }, 2000);
            }).catch(err => {
                // Fallback for older browsers
                const textArea = document.createElement('textarea');
                textArea.value = code;
                textArea.style.position = 'fixed';
                textArea.style.opacity = '0';
                document.body.appendChild(textArea);
                textArea.select();
                document.execCommand('copy');
                document.body.removeChild(textArea);
                
                alert('‚úÖ Code wurde kopiert!');
            });
        }
        
        // Per WhatsApp teilen
        function shareViaWhatsApp() {
            const codeElement = document.getElementById('syncCode');
            const code = codeElement.textContent;
            
            if (code === 'Generiere erst einen Wochenplan...') {
                alert('‚ùå Bitte erst einen Wochenplan generieren!');
                return;
            }
            
            const message = `üçΩÔ∏è *Wochenessensplan Sync-Code*\n\nHier ist unser Essensplan f√ºr diese Woche:\n\n${code}\n\n_F√ºge diesen Code in der App unter "Synchronisieren" ein._`;
            const whatsappUrl = `https://wa.me/?text=${encodeURIComponent(message)}`;
            
            window.open(whatsappUrl, '_blank');
        }
        
        // Native Share API
        function shareNative() {
            const codeElement = document.getElementById('syncCode');
            const code = codeElement.textContent;
            
            if (code === 'Generiere erst einen Wochenplan...') {
                alert('‚ùå Bitte erst einen Wochenplan generieren!');
                return;
            }
            
            const shareText = `üçΩÔ∏è Wochenessensplan Sync-Code\n\n${code}\n\nF√ºge diesen Code in der App unter "Synchronisieren" ein.`;
            
            // Check if Web Share API is available
            if (navigator.share) {
                navigator.share({
                    title: 'Wochenessensplan',
                    text: shareText
                }).then(() => {
                    console.log('Erfolgreich geteilt');
                }).catch((error) => {
                    console.log('Teilen abgebrochen:', error);
                });
            } else {
                // Fallback: Copy to clipboard
                navigator.clipboard.writeText(shareText).then(() => {
                    alert('‚úÖ Code wurde kopiert! Du kannst ihn jetzt in einer beliebigen App teilen.');
                });
            }
        }
        
        // Sync Code importieren
        function importSyncCode() {
            const input = document.getElementById('syncInput');
            const code = input.value.trim();
            const statusDiv = document.getElementById('syncStatus');
            
            if (!code) {
                statusDiv.innerHTML = '<div class="sync-error">‚ùå Bitte einen Code einf√ºgen!</div>';
                return;
            }
            
            try {
                // Decode from base64
                const jsonString = decodeURIComponent(atob(code));
                const syncData = JSON.parse(jsonString);
                
                // Validate data
                if (!syncData.version || !syncData.weekPlan) {
                    throw new Error('Ung√ºltiger Sync-Code');
                }
                
                // Import data
                currentWeekPlan = syncData.weekPlan;
                shoppingListData = syncData.shoppingList || {};
                preferences = syncData.preferences || preferences;
                weeksSincePizza = syncData.weeksSincePizza || weeksSincePizza;
                
                // Save preferences
                localStorage.setItem('mealPlannerPrefs', JSON.stringify(preferences));
                
                // Update displays
                loadPreferences();
                displayWeekOverview();
                displayShoppingList();
                displayRecipes();
                
                // Show success
                statusDiv.innerHTML = '<div class="sync-success">‚úÖ Wochenplan erfolgreich importiert!</div>';
                
                // Update sync info
                const syncInfo = document.getElementById('syncInfo');
                const now = new Date();
                syncInfo.textContent = `Synchronisiert: ${now.toLocaleDateString('de-CH')} um ${now.toLocaleTimeString('de-CH', {hour: '2-digit', minute: '2-digit'})}`;
                
                // Check if it's pizza week
                const hasPizza = currentWeekPlan.some(day => day.dinner.special === 'pizza-weekend');
                if (hasPizza) {
                    setTimeout(() => {
                        alert('üçï Pizza-Wochenende wurde importiert! Freitag und Samstag gibt es Pizza!');
                    }, 500);
                }
                
                // Clear input
                input.value = '';
                
                // Close modal after 2 seconds
                setTimeout(() => {
                    closeSyncDialog();
                }, 2000);
                
            } catch (error) {
                statusDiv.innerHTML = `<div class="sync-error">‚ùå Fehler beim Importieren: ${error.message}</div>`;
            }
        }
        
        // Aus Zwischenablage einf√ºgen
        function pasteFromClipboard() {
            navigator.clipboard.readText().then(text => {
                document.getElementById('syncInput').value = text;
                // Show visual feedback
                const btn = event.target;
                const originalText = btn.innerHTML;
                btn.innerHTML = '‚úÖ Eingef√ºgt!';
                
                setTimeout(() => {
                    btn.innerHTML = originalText;
                }, 1500);
            }).catch(err => {
                // Fallback for browsers that don't support clipboard API
                alert('Bitte manuell einf√ºgen (Strg+V oder Cmd+V)');
            });
        }
        
        // Close modal when clicking outside
        window.onclick = function(event) {
            const modal = document.getElementById('syncModal');
            if (event.target === modal) {
                closeSyncDialog();
            }
        }
    </script>
</body>
</html>
